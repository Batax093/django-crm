{
  "version": 2,
  "builds": [
    {
      "src": "djcrm/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    },
    {
      "src": "runserver.sh",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "static_root/$1"
    },
    {
      "src": "/(.*)",
      "dest": "runserver.sh"
    }
  ]
}
