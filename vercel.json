{
  "version": 2,
  "builds": [
    {
      "src": "waitress-serve --port $PORT djcrm.wsgi:application",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "waitress-serve"
    }
  ]
}
