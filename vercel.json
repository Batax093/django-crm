{
  "version": 2,
  "builds": [
    {
      "src": "djcrm/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLamdaSize" : "15mb", "runtime": "python3.11"}
    },
    {
      "src": "runserver.sh",
      "use": "@vercel/static_root",
      "config": {
        "distDir": "static_root"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "djcrm/wsgi.py"
    }
  ]
}
